#!/usr/bin/expect -f

set timeout 1

expect_after {
    timeout { exit -1 }
}

spawn lmi
expect "lmi> "
if {[llength $argv] > 0} {
    send "[lindex $argv 0]\r"
    expect {
        timeout { exit -2 }
        "[lindex $argv 1]"
    }
    expect "lmi> "
}
close
catch wait result
exit [lindex $result 3]
